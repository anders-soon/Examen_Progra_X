@model Dollar.Models.reportes

@{
    ViewBag.Title = "Create";
}

<h2>Create</h2>


@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()

    <div class="form-horizontal">
        <h4>reportes</h4>
        <hr />
        @Html.ValidationSummary(true, "", new { @class = "text-danger" })
        <div class="form-group">
            @Html.LabelFor(model => model.clientes, "clientes", htmlAttributes: new { @class = "control-label col-md-2" })
            <div class="col-md-10">
                @Html.DropDownList("clientes", null, htmlAttributes: new { @class = "form-control" })
                @Html.ValidationMessageFor(model => model.clientes, "", new { @class = "text-danger" })
            </div>
        </div>

        <div class="form-group">
            @Html.LabelFor(model => model.servicios, "servicios", htmlAttributes: new { @class = "control-label col-md-2" })
            <div class="col-md-10">
                @Html.DropDownList("servicios", null, htmlAttributes: new { @class = "form-control" })
                @Html.ValidationMessageFor(model => model.servicios, "", new { @class = "text-danger" })
            </div>
        </div>

        <div class="form-group">
            <div class="col-md-offset-2 col-md-10">
                <input type="submit" value="Create" class="btn btn-default" />
            </div>
        </div>
    </div>
}

<div>
    <p><a class="btn btn-warning" href="https://localhost:44352/Home/Contact">Regresar A registro</a></p>
</div>

@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
